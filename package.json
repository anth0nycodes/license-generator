{
  "name": "@anth0nycodes/license-generator",
  "version": "0.1.3",
  "description": "An open-source CLI tool to generate licenses for your repository.",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "publish:npm": "pnpm publish --access public"
  },
  "main": "dist/index.js",
  "bin": {
    "generate-license": "./dist/index.js"
  },
  "keywords": [
    "license",
    "license-generator",
    "license-cli",
    "cli",
    "open-source",
    "oss",
    "node",
    "npm",
    "npx",
    "mit",
    "apache-2.0",
    "boilerplate",
    "project-setup",
    "scaffold"
  ],
  "author": "Anthony Hoang",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anth0nycodes/license-generator.git"
  },
  "bugs": {
    "url": "https://github.com/anth0nycodes/license-generator/issues"
  },
  "homepage": "https://github.com/anth0nycodes/license-generator#readme",
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "inquirer": "^13.1.0",
    "picocolors": "^1.1.1"
  }
}
